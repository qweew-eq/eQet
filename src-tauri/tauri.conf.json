{
  "package": {
    "productName": "eQet",
    "version": "0.1.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "net.eew.eqet",
      "icon": [
        "icons/icon.png"
      ]
    },
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "title": "eQet",
        "width": 800,
        "height": 600,
        "resizable": true
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "distDir": "../dist",
    "devPath": "http://localhost:5173"
  }
}
